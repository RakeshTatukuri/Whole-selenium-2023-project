public class TestListeners {

	WebDriver driver;
	@Test
	public void test1() {
		driver=new ChromeDriver();
		driver.get("https://www.google.com/");
		Assert.assertTrue(driver.getTitle().contains("google"),"Test failed");
		System.out.println("Test passed");
	}
	
	@Test
	public void test2() {
		
	}
}
